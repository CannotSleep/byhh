
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>标引编辑</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="icon" href="images/favicon.ico" type="image/ico" />
    <link href="~/Content/webuploader/webuploader.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/layui/layui/css/layui.css" media="all">

    <style>

        @* body {
            text-align: center;
        }

        .row {
            margin-top: 10px;
            border-bottom: 1px solid #ededed;
            padding-bottom: 10px;
        }

        .col-sm-5 {
            text-align: left;
        }

        .content {
            margin-top: 30px;
            margin-bottom: 30px;
        }

            .content.tree {
                margin-left: 10%;
            }

        .col-sm-2 {
        }

        .contentinput {
            width: 80%;
            border-radius: 3px;
            border: 1px solid #000;
            height: 30px;
            padding-left: 20px;
        }

            /*发布日期*/
            .contentinput.fb {
                width: 100% !important;
                padding-left: 0px;
            }
            /*实施日期*/
            .contentinput.ss {
                width: 100% !important;
                padding-left: 0px;
            }

        .bixu {
            color: red;
        }

        .shuxingm {
            color: #000;
            font-weight: bold;
        }

        /********************************/
        h1 {
            color: #159def;
        }


        .ctable {
            width: 80%;
            margin-left: 10%;
        }

            .ctable tr {
                border: 1px solid #000;
            }

            .ctable td {
                border: 1px solid #000;
                font-size: 22px;
                height: 45px;
            }

        .ctinput {
            width: 100%;
            border: none;
            height: 40px;
        }

        .ctd {
            font-size: 16px;
            font-weight: bold;
        }

            .ctd .bitian {
                color: red;
            }*@
    </style>
</head>
<body>
    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>标引编辑</legend>
    </fieldset>
    <div class="content">
        <form id="standard" class="layui-form" action="">

            <div class="layui-form-item" style="display:none">
                <div class="layui-inline">
                    <label class="layui-form-label">标准ID</label>
                    <div class="layui-input-inline">
                        <input id="Id" type="text" name="Id" lay-verify="required" lay-reqtext="标准号必填"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">标准号</label>
                    <div class="layui-input-inline">
                        <input  type="text" name="StandNum" lay-verify="required" lay-reqtext="标准号必填"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>


            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">发布日期</label>
                    <div class="layui-input-inline">
                        <input type="text"  name="isd" id="date"   autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">实施日期</label>
                    <div class="layui-input-inline">
                        <input type="text" name="efd" id="date1"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">废止日期</label>
                    <div class="layui-input-inline">
                        <input type="text" name="AbolitionDate" id="date2"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">中文名称</label>
                    <div class="layui-input-inline">
                        <input type="text" name="cn" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">英文名称</label>
                    <div class="layui-input-inline">
                        <input type="text" name="en" autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">发布单位</label>
                    <div class="layui-input-inline">
                        <input type="text" name="PublishingUnit"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">批准单位</label>
                    <div class="layui-input-inline">
                        <input type="text" name="ApprovedUnit"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">提出单位</label>
                    <div class="layui-input-inline">
                        <input type="text" name="ProposedUnit"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">起草单位</label>
                    <div class="layui-input-inline">
                        <input type="text" name="DraftingUnit"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">替代标准</label>
                    <div class="layui-input-inline">
                        <input type="text" name="SubstituteStandard"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">被替代标准</label>
                    <div class="layui-input-inline">
                        <input type="text" name="SubstitutedStandard"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">起草人</label>
                    <div class="layui-input-inline">
                        <input type="text" name="zb_a001"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item layui-form-text">
                <label class="layui-form-label">适用范围</label>
                <div class="layui-input-block">
                    <textarea  name="scope" class="layui-textarea"></textarea>
                </div>
            </div>


            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">CCS</label>
                    <div class="layui-input-inline">
                        <input type="text" name="ccs"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">ICS</label>
                    <div class="layui-input-inline">
                        <input type="text" name="ics"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">GBN(国标号)</label>
                    <div class="layui-input-inline">
                        <input type="text" name="gbn"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">页数</label>
                    <div class="layui-input-inline">
                        <input type="text" name="Pages"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">备案号</label>
                    <div class="layui-input-inline">
                        <input type="text" name="RecordNumber"  autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>


            <div class="layui-form-item layui-form-text">
                <label class="layui-form-label">前言</label>
                <div class="layui-input-block">
                    <textarea placeholder="请输入内容" name="preface" class="layui-textarea"></textarea>
                </div>
            </div>


            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">修改日期</label>
                    <div class="layui-input-inline">
                        <input type="text" name="ModifyDate" id="date3"  autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">创建日期</label>
                    <div class="layui-input-inline">
                        <input type="text" name="CreateTime" id="date4" autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>


            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">题录选择</label>
                    <div class="layui-input-inline">
                        <select id="TableName" name="TableName" lay-verify="required" lay-reqtext="题录类型必选" lay-search="">
                            <option value="">直接选择或搜索选择</option>
                        </select>
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <label class="layui-form-label">标准状态</label>
                <div class="layui-input-block">
                    <input type="radio" name="StandStatus" value="现行" title="现行" checked="">
                    <input type="radio" name="StandStatus" value="废止" title="废止">
                    <input type="radio" name="StandStatus" value="未生效" title="未生效">
                </div>
            </div>

            <div class="layui-form-item">
                <label class="layui-form-label">是否免费</label>
                <div class="layui-input-block">
                    <input type="checkbox" name="isfree" lay-skin="switch" lay-text="是|否">
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">电子文本来源</label>
                @*<div class="layui-upload-drag" id="test10">
                <i class="layui-icon"></i>
                <p>点击上传，或将文件拖拽到此处</p>
                <div class="layui-hide" id="uploadDemoView">
                    <hr>
                    <img src="" alt="上传成功后渲染" style="max-width: 196px">
                </div>
                <button style="display:none" type="button" class="layui-btn" id="test9">开始上传</button>
            </div>*@
                <div class="layui-upload">
                    <button type="button" class="layui-btn layui-btn-normal" id="test8">选择文件</button>
                    <button type="button" style="display:none" class="layui-btn" id="test9">开始上传</button>
                    <span id="filename"></span>
                </div>
            </div>


            <div class="layui-form-item" style="border:1px solid #f2f2f2;border-radius:3px;">
                <label class="layui-form-label">规范性引用文件</label>
                <table class="layui-table" style="width:80%;" lay-even="" lay-skin="nob">
                    <colgroup>
                        <col width="150">
                        <col>
                        <col width="150">
                    </colgroup>
                    <thead>
                        <tr>
                            <th>类型</th>
                            <th>规范性引用文件条目</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody id="zguifan">
                    </tbody>
                </table>
            </div>


            <div class="layui-form-item" style="border:1px solid #f2f2f2;border-radius:3px;">
                <label class="layui-form-label">附录</label>
                <table class="layui-table" style="width:80%;" lay-even="" lay-skin="nob">
                    <colgroup>
                        <col width="150">
                        <col width="150">
                        <col>
                        <col width="150">
                    </colgroup>
                    <thead>
                        <tr>
                            <th>类型</th>
                            <th>附录编号</th>
                            <th>附录内容</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody id="zfulu">
                    </tbody>
                </table>
            </div>


            <div class="layui-form-item" style="border:1px solid #f2f2f2;border-radius:3px;">
                <label class="layui-form-label">参考文献</label>
                <table class="layui-table" style="width:80%;" lay-even="" lay-skin="nob">
                    <colgroup>
                        <col width="150">
                        <col>
                        <col width="150">
                    </colgroup>
                    <thead>
                        <tr>
                            <th>类型</th>
                            <th>参考文献条目</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody id="zcankao">
                    </tbody>
                </table>
            </div>

            <div class="layui-form-item" style="border:1px solid #f2f2f2;border-radius:3px;">
                <label class="layui-form-label">术语</label>
                <table class="layui-table" style="width:80%;" lay-even="" lay-skin="nob">
                    <colgroup>
                        <col width="100">
                        <col width="100">
                        <col width="100">
                        <col width="100">
                        <col width="100">
                        <col width="100">
                        <col width="100">
                        <col>
                        <col width="100">
                    </colgroup>
                    <thead>
                        <tr>
                            <th>类型</th>
                            <th>条目编号</th>
                            <th>术语名称</th>
                            <th>英文名称</th>
                            <th>术语定义</th>
                            <th>注</th>
                            <th>例</th>
                            <th>图表</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody id="zshuyu">
                    </tbody>
                </table>
            </div>

            <div class="layui-form-item" style="border:1px solid #f2f2f2;border-radius:3px;">
                <label class="layui-form-label">目次</label>
                <table class="layui-table" style="width:80%;" lay-even="" lay-skin="nob">
                    <colgroup>
                        <col width="150">
                        <col width="150">
                        <col>
                        <col width="150">
                    </colgroup>
                    <thead>
                        <tr>
                            <th>类型</th>
                            <th>目次条目编号</th>
                            <th>目次条目内容</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody id="zmuci">
                    </tbody>
                </table>
            </div>

            <div class="layui-form-item" style="border:1px solid #f2f2f2;border-radius:3px;">
                <label class="layui-form-label">结构化内容</label>
                <table class="layui-table" style="width:80%;" lay-even="" lay-skin="nob">
                    <colgroup>
                        <col width="150">
                        <col width="150">
                        <col width="150">
                        <col width="150">
                        <col width="200">
                        <col>
                        <col width="150">
                    </colgroup>
                    <thead>
                        <tr>
                            <th>类型</th>
                            <th>结构级别</th>
                            <th>结构编号</th>
                            <th>结构标题</th>
                            <th>结构内容</th>
                            <th>结构图表</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody id="zjiegou">
                    </tbody>
                </table>
            </div>

            <div class="layui-form-item">
                <div class="layui-input-block">
                    <button type="button" id="addguifan" class="layui-btn layui-btn-normal">增加规范性引用文件</button>
                    <button type="button" id="addfulu" class="layui-btn layui-btn-normal">增加附录</button>
                    <button type="button" id="addcankao" class="layui-btn layui-btn-normal">增加参考文献</button>
                    <button type="button" id="addshuyu" class="layui-btn layui-btn-normal">增加术语</button>
                    <button type="button" id="addmuci" class="layui-btn layui-btn-normal">增加目次</button>
                    <button type="button" id="addjishu" class="layui-btn layui-btn-normal">增加结构化内容</button>
                </div>
            </div>
            <div class="layui-form-item">
                <div class="layui-input-block">
                    <button type="submit" class="layui-btn" lay-submit="" lay-filter="demo1">立即提交</button>
                    <button type="reset" class="layui-btn layui-btn-primary">重置</button>
                </div>
            </div>
        </form>



