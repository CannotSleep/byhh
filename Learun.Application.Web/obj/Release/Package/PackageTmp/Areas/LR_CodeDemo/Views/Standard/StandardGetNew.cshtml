
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>标准追踪</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="~/Content/layui/layui/css/layui.css" media="all">
    <!-- 注意：如果你直接复制所有代码到本地，上述css路径需要改成你本地的 -->
    <style>
        .layui-form-item {
            margin-bottom: 5px;
            clear: both;
        }
        .layui-form-item .layui-input-inline {
            float: left;
            width: 150px;
            margin-right: 10px;
        }
        .layui-form-item .layui-inline {
            margin-bottom: 0px;
            margin-right: 1px;
        }
        .layui-btn-sm {
            height: 30px;
            line-height: 30px;
            padding: 0 10px;
            font-size: 12px;
            margin-top:3px;
        }
    </style>
</head>
<body>

    <div class="layui-container">

        <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
            <legend>标准时效性确认</legend>
        </fieldset>

        <div class="layui-row">
            <div class="layui-col-xs3">
                <button type="button" class="layui-btn layui-btn-sm layui-btn" id="testyy"><i class="layui-icon"></i>上传文件</button>
                <button type="button" id="track" class="layui-btn layui-btn-sm layui-btn">开始查证</button>
            </div>
        </div>
        <div class="layui-row">
            <div class="layui-col-xs12">
                <table class="layui-hide" id="demo" lay-filter="demo"></table>
            </div>
        </div>
        <script type="text/html" id="toolbarDemo">
            <div class="layui-btn-container">
                <button class="layui-btn layui-btn-sm" lay-event="getCheckData">导出选中行数据</button>
            </div>
        </script>

        <script type="text/html" id="barDemo">
            <a class="layui-btn layui-btn-xs" lay-event="edit">编辑</a>
            <a class="layui-btn layui-btn-danger layui-btn-xs" lay-event="del">删除</a>
        </script>

        <script type="text/html" id="mbarDemo">
            <a class="layui-btn layui-btn-xs" lay-event="confirm">确认</a>
        </script>

        <script type="text/html" id="usernameTpl">
            {{#  if(d.Csta === 0){ }}
            <span class="layui-badge">未通过</span>
            {{#  } else { }}
            <span class="layui-badge layui-bg-green">查证通过</span>
            {{#  } }}
        </script>
        <div id="res">

        </div>
    </div>
    
    <script src="~/Content/jquery/jquery-1.10.2.min.js"></script>
    <script src="~/Content/bootstrap/bootstrap.min.js"></script>
    <script src="~/Content/layui/layui/layui.js" charset="utf-8"></script>
    @Html.AppendJsFile("/Areas/LR_CodeDemo/Views/Standard/StandardGetNew.js")

</body>
</html>
